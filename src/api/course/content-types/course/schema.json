{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "singularName": "course",
    "pluralName": "courses",
    "displayName": "course",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true
    },
    "content": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "dynamiczone",
      "components": [
        "coursecontent.text",
        "coursecontent.image",
        "coursecontent.video",
        "coursecontent.external-video",
        "coursecontent.quiz",
        "coursecontent.pagebreaker"
      ]
    },
    "icon_image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "coursecategory": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::coursecategory.coursecategory",
      "inversedBy": "courses"
    },
    "order": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "integer"
    },
    "course_progresses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::course-progress.course-progress",
      "mappedBy": "course"
    },
    "course_unit_states": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::course-unit-state.course-unit-state",
      "mappedBy": "course"
    },
    "course_read_logs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::course-read-log.course-read-log",
      "mappedBy": "course"
    }
  }
}
